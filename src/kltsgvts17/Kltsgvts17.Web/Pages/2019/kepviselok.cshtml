@page
@model Kltsgvts17.Web.kepviselokModel
@{
    ViewData["Title"] = "kepviselok";
}

<div class="text-center">

    <h1 class="align-content-md-start">Képviselői keret 2019 Q4</h1>
    <div class="row">
        <div class="col-lg-12">
            <p>
                <strong>Az alábi táblázatban a képviselők által 2019. utolsó negyedévében kiosztott támogatások láthatóak.</strong>
            </p>
            <p>Éves keret: 10 millió Ft</p>
            <div class="card shadow mb-4">
                <div class="card-header">
                    <h3>4. negyedév</h3>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-bordered compact" id="dataTableKepviselok" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>Választókerület</th>
                                    <th>Címzettek</th>
                                    <th>Megvalósított feladat</th>
                                    <th>Összeg</th>
                                </tr>
                            </thead>
                            <tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

@section Scripts {
    <!-- Page level plugins -->
    <!-- Page level custom scripts -->
    <script src="~/js/datatables-2019-kepviselok.js"></script>
}




